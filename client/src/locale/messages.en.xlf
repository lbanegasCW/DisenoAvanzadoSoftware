      <trans-unit id="inicio.hero_title" datatype="html">
        <source>Comprá más inteligente</source>
        <target>Shop smarter</target>
      </trans-unit>
      <trans-unit id="inicio.hero_subtitle" datatype="html">
        <source>Compará precios en tiempo real de los principales supermercados</source>
