        <source>{totalResultados, plural, =0 {0 resultados} =1 {1 resultado} other {<x id="INTERPOLATION" equiv-text="# resultados"/> resultados}}</source>
        <target>{VAR_PLURAL, plural, =0 {0 results} =1 {1 result} other {# results}}</target>
      </trans-unit>
